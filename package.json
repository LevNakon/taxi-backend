{
  "name": "taxi-backend",
  "version": "1.0.0",
  "description": "Backend for internal project.",
  "main": "index.js",
  "scripts": {
    "test": "nyc mocha --timeout 50000",
    "start": "node app.js" 
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LevNakon/taxi-backend.git"
  },
  "author": "Lev Nakonechnyy",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LevNakon/taxi-backend/issues"
  },
  "homepage": "https://github.com/LevNakon/taxi-backend#readme",
  "eslintConfig": {
    "extends": "airbnb-base",
    "env": {
        "es6": true,
        "browser": true
    },
    "rules": {
        "brace-style": [
            "error",
            "stroustrup"
        ],
        "comma-dangle": [
            "error",
            "never"
        ],
        "no-unused-vars": [
            "warn"
        ],
        "no-var": [
            "off"
        ],
        "one-var": [
            "off"
        ]
    }
},
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "express-validator": "^6.0.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^7.11.0",
    "pg-hstore": "^2.3.3",
    "pool.socket.io": "0.0.1",
    "sequelize": "^5.8.12",
    "socket.io": "^2.2.0",
    "swagger-editor": "^3.6.31",
    "swagger-jsdoc": "^3.3.0",
    "swagger-ui-express": "^4.0.7",
    "yaml": "^1.6.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.2",
    "jsdoc": "^3.6.2",
    "mocha": "^6.1.4",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1",
    "sinon": "^7.3.2"
  }
}
